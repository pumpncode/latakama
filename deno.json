		"generate-prefixes-2": "deno run -R -W ./scripts/generate-prefixes-2.js",
		"convert-mathematica-prefixes-to-markdown": "deno run -R -W ./scripts/convert-mathematica-prefixes-to-markdown.js"
	},
	"license": "MIT",
	"imports": {
		"@/": "./",
		"@cliffy/ansi": "jsr:@cliffy/ansi@^1.0.0-rc.7",
		"@pumpn/eslint-config": "jsr:@pumpn/eslint-config@^16.0.0",
		"@radashi-org/radashi": "jsr:@radashi-org/radashi@^12.3.4",
		"@std/assert": "jsr:@std/assert@1",
		"@std/fs": "jsr:@std/fs@^1.0.4",
		"@std/path": "jsr:@std/path@^1.0.6",
		"@valibot/valibot": "jsr:@valibot/valibot@^1.0.0-rc.2",
		"es-hangul": "npm:es-hangul@^2.3.1",
